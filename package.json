{
  "name": "prishas-precious-projectiles",
  "version": "1.1.2",
  "description": "A Foundry VTT module for Pathfinder 2E adding custom ammunition types",
  "main": "dist/module.js",
  "type": "module",
  "scripts": {
    "build": "node scripts/build.mjs",
    "build:watch": "nodemon",
    "server:init": "node scripts/server-init.mjs",
    "server:cleanup": "node scripts/server-cleanup.mjs",
    "server": "node scripts/server.mjs",
    "version:bump": "node scripts/version-bump.mjs",
    "ts:build": "tsc && npm run minify",
    "ts:watch": "tsc --watch",
    "minify": "terser dist/module.js -o dist/module.js --compress --mangle",
    "dev": "npm run ts:build && npm run build",
    "compendium:extract": "node scripts/extract-compendium.mjs",
    "compendium:pack": "node scripts/pack-compendium.mjs",
    "ammo:build": "node scripts/build-ammo.mjs",
    "ammo:merge": "node scripts/merge-ammo.mjs"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "classic-level": "^2.0.0",
    "nodemon": "^3.0.2",
    "terser": "^5.24.0",
    "typescript": "^5.3.3",
    "uuid62": "^1.0.0",
    "js-yaml": "^4.1.0"
  },
  "keywords": [
    "foundry-vtt",
    "pathfinder-2e",
    "ammunition",
    "rpg"
  ],
  "author": "John Carney",
  "license": "MIT"
}
